<!--index.vtl-->
<div class="jumbotron">
  <h1>Star Realms Deck Manager</h1>
</div>

<div class="form-group">
  <form action="/" method="post">
    <div class="form-group">
      <label for="factionName">Enter Faction Name</label>
      <input id="factionName" name="factionName" type="text" required>
    </div>
  </form>
</div>

#if($factions)
#foreach ($faction in $factions)
<h2><a href="/factions/$faction.getFactionId()">$faction.getFactionName()</a></h2>
#end
#end

<!--index.vtl-->
